/* 导航栏1 */
.diy-graphic {
	background: #ffffff;
	padding: 5px;
}
.diy-graphic-container{
	width:100%;
	display:flex;
}
.diy-graphic .diy-graphic-list {
	width: 23%;
	background: #efeff4;
}
.diy-graphic .diy-graphic-list:nth-child(odd) {
	margin-right: 2.65%;
}
.diy-graphic .diy-graphic-list .main-title {
	font-size: 14px;
	color:#333333;
	text-align: center;
	margin-top: 6px;
	width: 100%;
	height: 20px;
	line-height: 20px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.diy-graphic .diy-graphic-list .subhead {
	font-size: 12px;
	color: #606060;
	text-align: center;
	width: 100%;
	height: 18px;
	line-height: 18px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.diy-graphic .diy-graphic-list .img {
	text-align: center;
	margin-top: 2px;
	margin-bottom: 2px;
	height: 56px;
}
.diy-graphic .diy-graphic-list .img img{
	width: 56px;
	height: 100%;
}
.diy-graphic .diy-graphic-list:nth-child(even) {
	margin-right: 2.65%;
}
.diy-graphic .diy-graphic-list:nth-child(4n+0) {
	margin-right: 0;
}
/*导航栏*/
.diy-navs{
	background: #a2d9f2;
}
